/*********cornersssssssssssssssss*************/
.topleftconcave {
    position:absolute;
    background:url(images/bg.jpg);
    width:28px;
    height:22px;
    border-bottom-right-radius:500px;
    left:0;
    top:0;
}
.botleftconcave {
    position:absolute;
    background:url(images/bg.jpg);
    width:25px;
    height:20px;
    bottom:0;
    left:0;
    border-top-right-radius:500px;
}

.toprightconcave {
    position:absolute;
    background: url(images/bg.jpg);
    width:25px;
    height:20px;
    right:0;
    border-bottom-left-radius:500px;
}
.botrighttconcave {
    position:absolute;
    background:url(images/bg.jpg);
    width:25px;
    height:20px;
    bottom:0;
    right:0;
    border-top-left-radius:500px;
}

